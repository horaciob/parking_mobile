<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.hbranciforte.trafficclient" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <uses-permission android:name="android.permission.INTERNET" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".NewParking"
            android:label="@string/title_activity_new_parking" >
        </activity>
        <activity
            android:name=".DataTraffic"
            android:label="@string/title_activity_data_traffic" >
        </activity>
        <activity
            android:name=".Resume"
            android:label="@string/title_activity_resume" >
        </activity>
    </application>

</manifest>
